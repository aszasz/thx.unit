#!/bin/sh
rm -rf src/*
generator -i definitions/Angle.hx -o "src/thx/unit/angle/\${type}.hx" -d definitions/angle.json -p values
generator -i definitions/Temperature.hx -o "src/thx/unit/temperature/\${type}.hx" -d definitions/temperature.json -p values
generator -i definitions/Unit.hx -o "src/thx/unit/\${unit}/\${type}.hx" -d definitions/unit.json -p values
generator -i definitions/UnitAlias.hx -o "src/thx/unit/\${unit}/\${type}.hx" -d definitions/unitalias.json -p values
